<div class='users_page'>

  <h2>Users Page</h2>


  <ul class='users_list'>
    <% @users.each do |user| %>
      <li>
        <% if user.name.present? %>
          <%=link_to user.name, user%>
        <% else %>
          'no user name'
        <% end %>
      </li>
    <% end %>
  </ul>





</div>